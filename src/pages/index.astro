---
import Equipo from '../components/Equipo.astro';
import Hero from '../components/Hero.astro';
import Quienes from '../components/Quienes.astro';
import Layout from '../layouts/Layout.astro';
import Contact from '../components/contact/Contact';
---

<Layout>
	<Hero />
	<Quienes />
	<Equipo />
	<Contact client:load/>
</Layout>

<style is:global>
	
	@font-face {
		font-family: 'Manrope';
		src: url('/public/fuentes/Manrope-Regular.ttf') format('truetype');
		font-weight: 400;
		font-style: normal;
		font-display: swap;
}
	@font-face {
		font-family: 'ManropeB';
		src: url('/public/fuentes/Manrope-Bold.ttf') format('truetype');
		font-weight: 800;
		font-style: normal;
		font-display: swap;
}
	

</style>